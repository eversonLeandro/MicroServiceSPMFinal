{
  "id": "MicroserviceSPM",
  "realm": "MicroserviceSPM",
  "enabled": true,
  "displayName": "MicroserviceSPM",
  "accessTokenLifespan": 300,
  "ssoSessionIdleTimeout": 1800,
  "ssoSessionMaxLifespan": 36000,
  "defaultRoles": [
    "default-roles-microservicespm"
  ],
  "clients": [
    {
      "id": "system-desktop-client-id",
      "clientId": "system-desktop",
      "name": "System Desktop Client",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "system-secret",
      "rootUrl": "http://localhost:8081",
      "baseUrl": "http://localhost:8081",
      "redirectUris": [
        "http://localhost:3000/*",
        "http://localhost:8081/*"
      ],
      "webOrigins": [
        "+",
        "http://localhost:8081"
      ],
      "adminUrl": "http://localhost:8081",
      "publicClient": false,
      "protocol": "openid-connect",
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": true,
      "fullScopeAllowed": true,
      "attributes": {
        "backchannel.logout.revoke.offline.tokens": "false",
        "backchannel.logout.session.required": "true"
      }
    }
  ],
  "roles": {
    "realm": [
      {
        "id": "default-roles-microservicespm-id",
        "name": "default-roles-microservicespm",
        "description": "Default roles for MicroserviceSPM realm",
        "composite": true,
        "clientRole": false,
        "containerId": "MicroserviceSPM"
      }
    ],
    "client": {
      "system-desktop": [
        {
          "id": "company-role-id",
          "name": "company",
          "description": "Rol para empresas",
          "composite": false,
          "clientRole": true,
          "containerId": "system-desktop-client-id"
        },
        {
          "id": "student-role-id",
          "name": "student",
          "description": "Rol para estudiantes",
          "composite": false,
          "clientRole": true,
          "containerId": "system-desktop-client-id"
        },
        {
          "id": "coordinator-role-id",
          "name": "coordinator",
          "description": "Rol para coordinadores",
          "composite": false,
          "clientRole": true,
          "containerId": "system-desktop-client-id"
        }
      ]
    }
  },
  "users": [
    {
      "id": "admin-user-id",
      "username": "admin",
      "enabled": true,
      "firstName": "Admin",
      "lastName": "User",
      "email": "admin@microservicespm.com",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "admin123",
          "temporary": false
        }
      ],
      "clientRoles": {
        "system-desktop": [
          "coordinator"
        ]
      }
    },
    {
      "id": "service-account-system-desktop-id",
      "username": "service-account-system-desktop",
      "enabled": true,
      "emailVerified": true,
      "serviceAccountClientId": "system-desktop",
      "credentials": [],
      "realmRoles": [
        "default-roles-microservicespm"
      ],
      "clientRoles": {
        "realm-management": [
          "view-users",
          "manage-users", 
          "realm-admin"
        ],
        "system-desktop": [
          "uma_protection"
        ]
      },
      "groups": []
    }
  ],
  "clientScopeMappings": {
    "realm-management": [
      {
        "client": "system-desktop",
        "roles": [
          "view-users",
          "manage-users",
          "realm-admin"
        ]
      }
    ]
  },
  "scopeMappings": [
    {
      "client": "system-desktop",
      "roles": [
        "default-roles-microservicespm"
      ]
    }
  ],
  "defaultDefaultClientScopes": [
    "web-origins",
    "profile",
    "roles",
    "email"
  ],
  "defaultOptionalClientScopes": [
    "address",
    "phone",
    "offline_access",
    "microprofile-jwt"
  ]
}